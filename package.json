{
  "name": "spigot-anti-piracy-csv-exporter",
  "version": "0.1.4",
  "description": "A helper utility to generate a csv export for the spigot-anti-piracy-backend",
  "bin": "./lib/csv-exporter.js",
  "main": "./lib/csv-exporter.js",
  "scripts": {
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 -p eslint",
    "lint": "eslint .",
    "prepublish": "npm run lint",
    "release": "standard-version",
    "start": "node lib/csv-exporter.js"
  },
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "timbru"
  },
  "dependencies": {
    "json2csv": "~3.7.3"
  },
  "devDependencies": {
    "conventional-changelog": "~1.1.0",
    "conventional-changelog-cli": "~1.2.0",
    "cz-customizable": "~4.0.0",
    "eslint": "~3.15.0",
    "standard-version": "~4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-csv-exporter",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-csv-exporter/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ]
}
