{
  "name": "spigot-anti-piracy-csv-exporter",
  "version": "0.2.0",
  "description": "A helper utility to generate a csv export for the spigot-anti-piracy-backend",
  "bin": "./src/csv-exporter.js",
  "main": "./src/csv-exporter.js",
  "scripts": {
    "lint": "eslint .",
    "prepublish": "npm run lint",
    "release": "standard-version",
    "start": "node ./src/csv-exporter.js"
  },
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "timbru"
  },
  "dependencies": {
    "json2csv": "~3.8.0"
  },
  "devDependencies": {
    "cz-customizable": "~5.0.0",
    "eslint": "~4.1.1",
    "standard-version": "~4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-csv-exporter",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-csv-exporter/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ]
}
